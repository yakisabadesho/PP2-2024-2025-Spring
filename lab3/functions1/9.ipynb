    "import math\n",
    "\n",
    "def sphere_volume(radius):\n",
    "    return (4/3) * math.pi * (radius ** 3)\n",
    "radius = float(input(\"Radius of the sphere: \"))\n",
    "volume = sphere_volume(radius)\n",
    "print(\"Volume of the sphere:\", volume)"
